	rm -f $(OBJ_RELEASE) $(OUT_RELEASE)
	rm -rf ../Game/bin
	rm -rf $(OBJDIR_RELEASE)/ALAGEngine/src/extlibs
	rm -rf $(OBJDIR_RELEASE)/ALAGEngine/src/gfx
	rm -rf $(OBJDIR_RELEASE)/ALAGEngine/src/core
	rm -rf $(OBJDIR_RELEASE)/ALAGEngine/src/gfx/iso
	rm -rf $(OBJDIR_RELEASE)/src/states
	rm -rf $(OBJDIR_RELEASE)
	rm -rf $(OBJDIR_RELEASE)/ALAGEngine/src/utils

